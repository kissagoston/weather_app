ities CitySearch={CitySearch}/>\r\n                </div>\r\n                <div className=\"showWeatherDetails\"  >\r\n                  {/* amit megkapunk useFetchnál adatot, átadjuk propsként */}\r\n                      <Showdata data={data} city={city} />\r\n                      \r\n                </div>\r\n            </div>\r\n\r\n       </div>\r\n     </div>\r\n         \r\n    </div>\r\n  );\r\n}\r\n\r\nexport default App;\r\n","C:\\Projects\\weather_app\\weather-app-javascript-calunnia\\weather-app\\src\\reportWebVitals.js",[],"C:\\Projects\\weather_app\\weather-app-javascript-calunnia\\weather-app\\src\\components\\othercities\\other-cities.js",[],"C:\\Projects\\weather_app\\weather-app-javascript-calunnia\\weather-app\\src\\components\\othercities\\citiesData.js",[],["118","119"],"C:\\Projects\\weather_app\\weather-app-javascript-calunnia\\weather-app\\src\\components\\show-data\\showdata.js",[],"C:\\Projects\\weather_app\\weather-app-javascript-calunnia\\weather-app\\src\\components\\favourites\\favourites.js",[],"C:\\Projects\\weather_app\\weather-app-javascript-calunnia\\weather-app\\src\\components\\usefecth\\usefetch.js",["120"],"import {useEffect} from 'react';\r\n//http://api.openweathermap.org/data/2.5/weather?q=Budapest&appid=88819b23426fc788e6079b2cca861c71\r\n//http://openweathermap.org/img/w/04d.png\r\n// var iconurl = \"http://openweathermap.org/img/w/\" + iconcode + \".png\";\r\n\r\n// APIendpoint - Innen származnak az adatok \r\nexport const  mainUrls={ URL : \"http://api.openweathermap.org/data/2.5/weather\",\r\n                         API_key : \"88819b23426fc788e6079b2cca861c71\"\r\n                        };\r\n\r\nexport const loadData = async (url,setData) =>{\r\n    const response = await fetch(url);\r\n    const data = await response.json();\r\n    //  console.log(data);\r\n    setData(data);\r\n}\r\n","C:\\Projects\\weather_app_cv\\weather_app\\weather-app\\src\\index.js",[],"C:\\Projects\\weather_app_cv\\weather_app\\weather-